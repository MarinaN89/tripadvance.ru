.wrapper{
    
    max-width: 1310px;
    margin: 0 auto ;
    margin-top: 64px;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.title{

}

.line{
    position:absolute;
    background: #F2F2F2;
    height: 38px;
    width: 100%;
    bottom: -16px;
}

.title__text1{
    font-size: 54px;
}

.title__block2{
    display: flex;
}

.title__text2{
    font-size: 22px;
}

.title__text3{
    font-size: 28px;
    color:yellow;
    margin-left: 6px;
}

.buttons{
    display:flex;
}
.button-left{
    padding: 24px 32px;
    background-color:darkslategrey;
    color:white;
    font-size: 24px;
}
.button-right{
    margin-left: 4px;
    padding: 24px 32px;
    background-color:darkslategrey;
    color:white;
    font-size: 24px;
}

.new{
    display: flex;
    flex-direction: column;
    width: 270px;
    margin-right: 50px;
    height: 100%;
    min-height: 100%;
    height: 360px;
    justify-content: space-between;
    align-items: stretch;
}

.new-block{

}

.new-block__text1{
    color:yellow;
}

.new-block__title{
    margin-bottom: 12px;
}

.new-block{
    margin-top: 0px;
}

.new-block__button{
    margin-top: 12px;
    display: flex;
}

.new-block__button-text{
    color: #ED3E3E;
    font-weight: 700;
}

.new-block__button-arrow{
    margin-left: 6px;
}

.new-separator{
    margin-top: 0px;
    margin-bottom: 0px;
    border-bottom: 1px solid lightgrey;
}


.wrapper-products{
    max-width: 1310px;
    margin: 0 auto ;
    margin-top: 48px;
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
    position: relative;

}

.products{
    position:relative;
    width: 100%;
    z-index: 1;
}

.product{
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    /*width: 320px;*/
    width:100%;
    height: auto;
    min-height:370px;
    margin-right: 0px;
    position: relative;
    padding-left: 12px;
    
}

.product-top{
    width: 100%;
    height: 165px;
    position: relative;
}

.product-top__background{
    width: 100%;
    height: 165px;
    position: absolute;
    top:0;
    left:0;
    z-index: -1;
}

.product-bottom{
    border-right: 1px solid lightgrey;
    border-left: 1px solid lightgrey;
    border-bottom: 1px solid lightgrey;
    height:100%;
}

.product:last-child{
    margin-right: 0;
}

.product-header{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 36px;
    height: 165px;
}

.product-header__title_icon{
    width:76px;
    margin-left:24px;
}
.product-header__title_text1{
    font-size: 16px;
}

.product-header__title_text2{
    margin-top: 12px;
    font-size: 14px;
}

.product-navigation{
    display: flex;
    justify-content: space-between;
    padding-top: 36px;
    padding-bottom: 24px;
    align-items: center;
    padding: 36px;
}

.product-navigation__button{
    background: #ED3E3E;
    color:white;
    width: 64px;
    border-radius: 24px;
    padding: 12px 24px;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 14px;
}

.product-navigation__service{
    margin-right:0px;
    font-size: 14px;
}

.product-description{
    font-size: 14px;
    padding: 0 36px 0px 36px;
    
}

.product-v{
    border-left:1px solid black;
    height: 24px;
    position: absolute;
    bottom: 20px;
    right: 20px;
}


@media (max-width:680px) {
    .wrapper{
        flex-direction: column;
        align-items: center;
        margin-left: 12px;
        margin-top: 24px;
    }
    .title{
        display: flex;
        flex-direction: column;
        align-items: center;
    }
    .title__text1{
        font-size: 24px;
        margin-bottom: 12px;;
    }

    .title__text2{
        font-size: 14px;
    }

    .buttons{
        margin-top: 12px;
    }
    .button-left{
        padding: 12px 16px;
        font-size: 12px;
    }

    .button-right{
        padding: 12px 16px;
        font-size: 12px;
    }

    .wrapper-products{
        flex-direction: column-reverse;
        align-items: center;
        margin-top: 24px;
    }

    .new{
        margin-left: 12px;
        margin-top: 24px;
    }

    .product{
        padding-right: 12px;
        padding-left: 12px;
    }

    .product-header{
        justify-content: center;
        align-items: center;
        flex-direction: column-reverse;
    }

    .product-header__title{
        text-align: center;
    }

    .product-header__title_icon{
        margin-left: 0px;
        margin-bottom:12px;
    }

    .product-bottom{
        display: flex;
        flex-direction: column-reverse;
        align-items: center;
        justify-content: flex-end;
        padding-top: 24px;
    }

    .product-navigation{
        flex-direction: column-reverse;
        padding: 18px;
    }

    .product-navigation__service{
        margin-bottom: 24px;
    }

    .product-description{
        text-align: center;
        font-size: 16px;
    }

    .product-header__title_text2{
        font-size: 16px;
    }
}

@media (max-width:1310px) {
    .wrapper{
        padding-right: 12px;
        padding-left: 12px;
    }
    .wrapper-products{
        padding-right: 12px;
        padding-left: 12px;
    }
}