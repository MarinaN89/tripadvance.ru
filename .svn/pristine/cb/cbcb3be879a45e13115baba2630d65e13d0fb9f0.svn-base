.wrapper{
    max-width: 1310px;
    margin:0 auto;
}

.group-logo{
    display: flex;
}

.logo-ta{
    display: flex;
    align-items: center;
    max-width: 224px;;
}

.logo-text{
    margin-left: 24px;
}

.logo-1c{
    display: flex;
    margin-left: 64px;
    align-items: center;
}

.company{
    margin-top: 24px;
}

.company-detail{
    margin-top:12px;
    font-size: 14px;
    text-decoration: underline;
    margin-bottom: 24px;
}

.company-text{
    font-size: 14px;
}

.company-text__margin-top{
    margin-top: 12px;
}

@media (max-width:680px){
    .group-logo{
        flex-direction: column;
        justify-content: center;
        align-items: center;
    }

    .logo-ta{
        max-width: 100%;
        display: flex;
        flex-direction: column;
    }

    .logo-1c{
        margin-top:12px;
        margin-left: 0px;
        max-width: 100%;
        display: flex;
        flex-direction: column;
    }

    .logo-text{
        margin-left:0px;
    }

    .company{
        display: flex;
        flex-direction: column;
        align-items: center;
    }
}